<div id="paso2">

  <% if (typeof persona !== 'undefined') { %>
    <h4><%= persona.pernombrecompleto %></h4>
  <% } %>

  <form method="post" class="form-horizontal" id="frm-paso1" action='paso3'>

    <div class="panel panel-default">
      <div class="panel-heading">Última inscripción a liceo:</div>
      <div class="panel-body">


      <% if (typeof inscripciones !== 'undefined') { %>
        <% inscripciones.forEach(function(inscripcion){ %>

          <div class="media">
            <div class="media-left">
                <div class="passepartout">
                  <img class="media-object" src="http://monkey.ces.edu.uy/imagenes/liceos/liceo03.jpg" alt="<%= inscripcion.DependId %>">
                </div>
            </div>
            <div class="media-body">
              <ul class="list-group">
                <li class="list-group-item"><h4 class="media-heading">Liceo: <%= inscripcion.DependId.DependDesc %><% if (inscripcion.DependId.DependNom) {%> <br><%= inscripcion.DependId.DependNom %><% } %></h4></li>
              </ul>
              <p style="text-align:center">Detalle de la inscripción:</p>
              <ul class="list-group">
                <li class="list-group-item"><b>Plan:</b> <%= inscripcion.PlanId.PlanNombre %></li>
                <li class="list-group-item"><b>TipoModalidadId:</b> <%= inscripcion.TipoModalidadId %></li>
                <li class="list-group-item"><b>CicloId:</b> <%= inscripcion.CicloId %></li>
                <li class="list-group-item"><b>GradoId:</b> <%= inscripcion.GradoId %></li>
                <li class="list-group-item"><b>OrientacionId:</b> <%= inscripcion.OrientacionId %></li>
                <li class="list-group-item"><b>OpcionId:</b> <%= inscripcion.OpcionId %></li>
                <li class="list-group-item"><b>InscriTurno:</b> <%= inscripcion.InscriTurno %></li>
              </ul>
            </div>
          </div>
        <% }) %>
      <% } %>

      </div>
    </div>

    <button type="submit" class="btn btn-primary" id="btn-paso1" name="paso1"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Atrás</button>
    <button type="submit" class="btn btn-primary" id="btn-paso3" name="paso3">Solicitar cambio de liceo <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>

    <div class="alert alert-danger" id="mensaje" style="display:none"></div>
    <script type="text/javascript">
      textoMensaje='<% if (typeof mensaje!=='undefined') { %><%= mensaje %><% } %>'
    </script>

  </form>
</div>
